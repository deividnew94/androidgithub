        <activity
            android:name="com.pe.sise.trackingtaxista.Activity_Inicio"
            android:label="@string/title_activity_activity__inicio" >
        </activity>
        <activity
            android:name="com.pe.sise.trackingtaxista.Activity_Registrate"
            android:label="@string/title_activity_activity__registrate" >
        </activity>
        <activity
            android:name="com.pe.sise.trackingtaxista.Activity_OlvideMiPassword"
            android:label="@string/title_activity_activity__olvide_mi_password" >
